{
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
    "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
        "arrowParens": "always",
        "htmlWhitespaceSensitivity": "strict",
  "fluid": false,
        "overrides": [
            {
                "files": "*.js",
                "options": {
                    "brace-style": "allman"
                }
            }
        ]
}